        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "@vercel/ncc": "^0.36.0",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jest": "^27.1.6",
        "jest": "^29.3.1",
        "jest-circus": "^29.3.1",
        "prettier": "^2.8.1",
